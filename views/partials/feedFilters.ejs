<form id="feedFilters" enctype="multipart/form-data" action="/feed" method="GET">
    <div class="row">
        <div class="col-3">
            <label for="timespan">Timespan:</label>
            <select name="timespan" class="form-control text-light bg-dark mb-5" id="timespanSelect">
                <option value="7">This Week</option>
                <option value="0">All Time</option>
                <option value="30">This Month</option>
                <option value="90">Last 90 Days</option>
            </select>
        </div>
        <div class="col-3">
            <label for="tech">Tech:</label>
            <select name="tech" class="form-control text-light bg-dark mb-5" id="techSelect">
                <option value="0">All Techs</option>
                <% users.forEach(user => { %>
                    <option value="<%= user._id %>"><%= user.userName %></option>
              <%  }) %>
            </select>
        </div>
        <div class="col-3 d-flex flex-row">
            <input type="submit" class="btn btn-primary mb-5 align-self-end" value="Reload">
        </div>
    </div>
</form>